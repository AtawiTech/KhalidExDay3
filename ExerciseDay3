//Ex6

import java.util.Arrays;

public class Ex6 {
    public static void main(String[] args) {
        int y[] = Numbers(3,4,5);
        System.out.println(Arrays.toString(y));
    }
    public static int [] Numbers(int A, int B, int C){
        int Threenumbers [] = {A,B,C};
        return Threenumbers;
    }
}
